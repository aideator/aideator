{
  "name": "aideator-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:quality && npm run test:unit && npm run test:e2e",
    "test:unit": "jest --testPathPattern=tests/unit --passWithNoTests",
    "test:integration": "jest --testPathPattern=tests/integration --passWithNoTests",
    "test:quality": "npm run type-check && npm run lint && npm run test:knip",
    "type-check": "tsc --noEmit",
    "test:knip": "knip --no-exit-code"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "1.2.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "1.2.2",
    "@radix-ui/react-select": "2.1.4",
    "@radix-ui/react-slot": "1.1.1",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "1.1.2",
    "@radix-ui/react-toast": "^1.2.14",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "next": "15.2.4",
    "next-themes": "^0.4.4",
    "postcss-load-config": "^6.0.1",
    "react": "^19",
    "react-dom": "^19",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "knip": "^5.61.3",
    "postcss": "^8.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
