{
  "name": "aideator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:quality && npm run test:unit && npm run test:integration",
    "test:all:coverage": "npm run test:quality && npm run test:unit:coverage && npm run test:integration:coverage",
    "test:unit": "jest --passWithNoTests",
    "test:unit:coverage": "jest --coverage --passWithNoTests",
    "test:integration": "jest --testPathPattern=tests/integration --passWithNoTests",
    "test:integration:coverage": "jest --testPathPattern=tests/integration --coverage --passWithNoTests",
    "test:quality": "npm run type-check && npm run test:knip",
    "type-check": "tsc --noEmit",
    "find-dead-code": "./find-dead-code.sh",
    "find-dead-code:js": "node find-dead-code.js",
    "find-dead-code:py": "python3 find-dead-code.py",
    "find-dead-code:ts": "npx ts-node find-dead-code-advanced.ts",
    "analyze:unused": "npx ts-unused-exports tsconfig.json --excludePathsFromReport=node_modules;.next;tests",
    "analyze:prune": "npx ts-prune --error",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "knip:production": "knip --production",
    "test:knip": "knip --no-exit-code"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-scroll-area": "^1.2.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.18.2",
    "lucide-react": "^0.525.0",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.53.2",
    "@radix-ui/react-slider": "^1.3.5",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "eslint-config-next": "15.2.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "knip": "^5.61.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "typescript": "^5"
  }
}
